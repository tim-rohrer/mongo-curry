{
 "name": "mongo-curry",
 "version": "0.1.1",
 "description": "A simple, curried approach to MongoDB for Node",
 "main": "dist/mongo-curry.js",
 "exports": {
  ".": "./dist/mongo-curry.js"
},
"types": "./dist/mongo-curry.d.ts",
"type": "module",
 "repository": "git@gitlab.com:Creplav/mongo-curry.git",
 "author": "Ben Doty <creplav@gmail.com>",
 "license": "GNU AGPLv3",
 "private": false,
 "scripts": {
  "babel-build": "babel src -d dist/",
  "eslint": "eslint --fix",
  "test": "yarn build && nyc riteway -r @babel/polyfill dist/**/**/*.test.js dist/**/*.test.js | tap-nirvana",
  "itest": "yarn build && nyc riteway  -r @babel/polyfill dist/**/**/*.int-test.js dist/**/*.int-test.js | tap-nirvana",
  "build:clean": "rm -rf dist/*",
  "build": "npm run build:clean && tsc --project tsconfig.json"
 },
 "husky": {
  "hooks": {
   "pre-commit": "yarn test && yarn eslint",
   "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }
 },
 "devDependencies": {
  "@babel/cli": "^7.13.0",
  "@babel/core": "^7.13.1",
  "@babel/plugin-transform-runtime": "^7.13.7",
  "@babel/polyfill": "^7.12.1",
  "@babel/preset-env": "^7.13.5",
  "@commitlint/cli": "^12.0.0",
  "@commitlint/config-conventional": "^12.0.0",
  "@tsconfig/node16": "^1.0.3",
  "@typescript-eslint/parser": "^5.30.6",
  "eslint": "^7.20.0",
  "husky": "^5.1.1",
  "nyc": "^15.1.0",
  "riteway": "^6.2.1",
  "tap-nirvana": "^1.1.0",
  "typescript": "^4.7.4"
 },
 "dependencies": {
  "mongodb": "^4.8.0"
 }
}
